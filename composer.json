{
  "name": "gategem/core",
  "description": "This is Lara Platform Core",
  "keywords": [
    "GateGem",
    "laravel",
    "core",
    "wordpress",
    "Lara Platform"
  ],
  "homepage": "https://github.com/GateGem/core",
  "license": "MIT",
  "authors": [
    {
      "name": "Nguyen Van Hau",
      "email": "nguyenvanhau.soft@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {},
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "GateGem\\Core\\": "src",
      "GateGem\\Core\\Database\\Factories\\": "database/factories",
      "GateGem\\Core\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GateGem\\Core\\Tests\\": "tests"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "GateGem\\Core\\CoreServiceProvider"
      ],
      "aliases": {
        "LaraCore": "GateGem\\Core\\Facades\\Core"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
